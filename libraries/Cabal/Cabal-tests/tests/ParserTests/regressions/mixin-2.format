cabal-version: 3.0
name:          mixin
version:       0

executable str-example
    main-is:        Main.hs
    hs-source-dirs: str-example
    build-depends:
        base,
        str-string,
        str-bytestring

    mixins:
        str-string (Str as Str.String),
        str-bytestring (Str as Str.ByteString)
